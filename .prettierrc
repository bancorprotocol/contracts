{
    "overrides": [
        {
            "files": "*.sol",
            "options": {
                "singleQuote": false,
                "explicitTypes": "always",
                "tabWidth": 4
            }
        }
    ],
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
